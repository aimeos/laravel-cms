{
	"name": "aimeos/laravel-cms",
	"description": "Laravel CMS",
	"keywords": ["laravel", "cms", "package"],
    "homepage": "https://laravel-cms.org",
	"type": "library",
	"license": "MIT",
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"laravel/framework": "~9.0||~10.0",
        "mll-lab/graphql-php-scalars": "^6.0",
		"nuwave/lighthouse": "dev-master",
		"kalnoy/nestedset": "~6.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\Cms\\": "src"
		},
		"files": [
			"src/helpers.php"
		],
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\Cms\\": "tests"
		},
		"classmap": [
			"tests"
		]
	},
    "extra": {
        "laravel": {
            "providers": [
                "Aimeos\\Cms\\CmsServiceProvider"
            ]
        }
    }
}
